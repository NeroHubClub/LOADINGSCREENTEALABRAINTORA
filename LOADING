progressBar.Parent = progressContainer

background.Parent = mainFrame
content.Parent = mainFrame

title.Parent = content
subtitle.Parent = content
systemMessage.Parent = content
progressContainer.Parent = content
percentage.Parent = content
warning.Parent = content

mainFrame.Parent = screenGui
screenGui.Parent = PlayerGui

local systemMessages = {
	"Checking executor compatibility...",
	"Validating game version...",
	"Checking required permissions...",
	"Verifying script environment...",
	"Scanning for missing dependencies...",
	"Ensuring required services are running...",
	"Preventing common execution errors...",
	"Do not reset character to avoid errors",
	"Please do not close or minimize Roblox",
	"Loading Brainrots data...",
	"Fetching mutation database...",
	"Initializing trait system...",
	"Injecting UI components...",
	"Synchronizing server data...",
	"Running security checks...",
	"Protecting script from detection...",
	"Encrypting runtime data...",
	"Final checks in progress...",
	"Almost done, please wait..."
}

local function startLoading()
	local LOAD_DURATION = 10
	local startTime = tick()
	local endTime = startTime + LOAD_DURATION
	local lastMessageTime = startTime
	local messageIndex = 1

	local bgTween = TweenService:Create(mainFrame, TweenInfo.new(0.5), {BackgroundTransparency = 0})
	bgTween:Play()

	wait(0.3)

	TweenService:Create(title, TweenInfo.new(0.6), {TextTransparency = 0}):Play()
	wait(0.1)
	TweenService:Create(subtitle, TweenInfo.new(0.6), {TextTransparency = 0}):Play()
	wait(0.1)
	TweenService:Create(systemMessage, TweenInfo.new(0.6), {TextTransparency = 0}):Play()
	wait(0.1)
	TweenService:Create(progressContainer, TweenInfo.new(0.6), {BackgroundTransparency = 0}):Play()
	wait(0.1)
	TweenService:Create(percentage, TweenInfo.new(0.6), {TextTransparency = 0}):Play()
	wait(0.1)
	TweenService:Create(warning, TweenInfo.new(0.6), {TextTransparency = 0}):Play()

	wait(0.5)

	systemMessage.Text = systemMessages[1]
	messageIndex = 2

	while tick() < endTime do
		local currentTime = tick()
		local elapsed = currentTime - startTime
		local progress = math.min(elapsed / LOAD_DURATION, 1)

		progressBar.Size = UDim2.new(progress, 0, 1, 0)

		local displayPercent = math.floor(progress * 100)
		percentage.Text = displayPercent .. "%"

		if currentTime - lastMessageTime >= 3.5 then
			systemMessage.Text = systemMessages[messageIndex]
			messageIndex = messageIndex % #systemMessages + 1
			lastMessageTime = currentTime

			if math.random(1, 10) == 1 then
				wait(math.random(0.5, 1.5))
			end
		end

		wait(0.05)
	end

	progressBar.Size = UDim2.new(1, 0, 1, 0)
	percentage.Text = "100%"
	systemMessage.Text = "Loading complete!"

	wait(1)

	TweenService:Create(title, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
	TweenService:Create(subtitle, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
	TweenService:Create(systemMessage, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
	TweenService:Create(percentage, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
	TweenService:Create(warning, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
	TweenService:Create(progressContainer, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
	TweenService:Create(progressBar, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()

	wait(0.5)

	TweenService:Create(mainFrame, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()

	wait(0.5)

	StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
	screenGui:Destroy()
end

RunService.Heartbeat:Wait()
spawn(startLoading)
