local warning = Instance.new("TextLabel")
warning.Name = "Warning"
warning.AnchorPoint = Vector2.new(0.5, 0.5)
warning.Size = UDim2.new(0.8, 0, 0, 50)
warning.Position = UDim2.new(0.5, 0, 0.75, 0)
warning.BackgroundTransparency = 1
warning.Text = "Do not close or minimize Roblox"
warning.TextColor3 = Color3.fromRGB(255, 180, 100)
warning.TextSize = 14
