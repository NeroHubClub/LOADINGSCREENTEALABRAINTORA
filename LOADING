local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")
local StarterGui = game:GetService("StarterGui")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "PremiumLoader"
screenGui.IgnoreGuiInset = true
screenGui.DisplayOrder = 999999
screenGui.ResetOnSpawn = false

local mainFrame = Instance.new("Frame")
mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(1, 0, 1, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 15)
mainFrame.BackgroundTransparency = 1
mainFrame.BorderSizePixel = 0

local background = Instance.new("Frame")
background.Name = "Background"
background.Size = UDim2.new(1, 0, 1, 0)
background.BackgroundColor3 = Color3.fromRGB(8, 8, 12)
background.BorderSizePixel = 0

local content = Instance.new("Frame")
content.Name = "Content"
content.Size = UDim2.new(1, 0, 1, 0)
content.BackgroundTransparency = 1

local title = Instance.new("TextLabel")
title.Name = "Title"
title.AnchorPoint = Vector2.new(0.5, 0.5)
title.Size = UDim2.new(0.8, 0, 0, 80)
title.Position = UDim2.new(0.5, 0, 0.3, 0)
title.BackgroundTransparency = 1
title.Text = "Steal A Brainrots Script"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.TextSize = 48
title.Font = Enum.Font.GothamBold
title.TextStrokeTransparency = 0.8
title.TextStrokeColor3 = Color3.fromRGB(30, 60, 255)
title.TextTransparency = 1

local subtitle = Instance.new("TextLabel")
subtitle.Name = "Subtitle"
subtitle.AnchorPoint = Vector2.new(0.5, 0.5)
subtitle.Size = UDim2.new(0.8, 0, 0, 30)
subtitle.Position = UDim2.new(0.5, 0, 0.37, 0)
subtitle.BackgroundTransparency = 1
subtitle.Text = "Created by: Nero"
subtitle.TextColor3 = Color3.fromRGB(200, 200, 220)
subtitle.TextSize = 20
subtitle.Font = Enum.Font.Gotham
subtitle.TextTransparency = 1

local systemMessage = Instance.new("TextLabel")
systemMessage.Name = "SystemMessage"
systemMessage.AnchorPoint = Vector2.new(0.5, 0.5)
systemMessage.Size = UDim2.new(0.8, 0, 0, 40)
systemMessage.Position = UDim2.new(0.5, 0, 0.5, 0)
systemMessage.BackgroundTransparency = 1
systemMessage.Text = "Initializing..."
systemMessage.TextColor3 = Color3.fromRGB(180, 220, 255)
systemMessage.TextSize = 18
systemMessage.Font = Enum.Font.RobotoMono
systemMessage.TextXAlignment = Enum.TextXAlignment.Center
systemMessage.TextTransparency = 1

local progressContainer = Instance.new("Frame")
progressContainer.Name = "ProgressContainer"
progressContainer.AnchorPoint = Vector2.new(0.5, 0.5)
progressContainer.Size = UDim2.new(0.6, 0, 0, 30)
progressContainer.Position = UDim2.new(0.5, 0, 0.6, 0)
progressContainer.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
progressContainer.BorderSizePixel = 0
progressContainer.ClipsDescendants = true
progressContainer.BackgroundTransparency = 1

local progressBar = Instance.new("Frame")
progressBar.Name = "ProgressBar"
progressBar.Size = UDim2.new(0, 0, 1, 0)
progressBar.BackgroundColor3 = Color3.fromRGB(30, 120, 255)
progressBar.BorderSizePixel = 0

local percentage = Instance.new("TextLabel")
percentage.Name = "Percentage"
percentage.AnchorPoint = Vector2.new(0.5, 0.5)
